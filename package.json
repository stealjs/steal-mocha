{
  "name": "steal-mocha",
  "version": "0.0.2",
  "description": "A package for Mocha and StealJS",
  "main": "steal-mocha",
  "system": {
    "npmIgnore": {
      "bower": true,
      "grunt": true,
      "grunt-cli": true,
      "grunt-contrib-copy": true,
      "grunt-contrib-watch": true,
      "steal": true,
      "steal-tools": true
    },
    "meta": {
      "mocha#./mocha": {
        "format": "global",
        "exports": "mocha",
        "deps": [
          "steal-mocha/add-dom"
        ]
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stealjs/steal-mocha.git"
  },
  "keywords": [
    "StealJS",
    "QUnit"
  ],
  "author": "Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stealjs/steal-mocha/issues"
  },
  "homepage": "https://github.com/stealjs/steal-mocha",
  "devDependencies": {
    "steal": "^0.7.0",
    "steal-tools": "^0.7.0",
    "bower": "1.3.8",
    "grunt": "~0.4.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-watch": "~0.3.0"
  },
  "dependencies": {
    "mocha": "^2.2.1"
  }
}
