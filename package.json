{
  "name": "steal-mocha",
  "version": "2.0.1",
  "description": "A package for Mocha and StealJS",
  "main": "steal-mocha",
  "steal": {
    "plugins": [
      "steal-css"
    ],
    "meta": {
      "mocha#./mocha": {
        "format": "global",
        "exports": "mocha",
        "deps": [
          "steal-mocha/add-dom"
        ]
      }
    }
  },
  "scripts": {
    "test": "npm run testee",
    "testee": "testee test/test.html --browsers firefox --reporter Spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stealjs/steal-mocha.git"
  },
  "keywords": [
    "StealJS",
    "Mocha"
  ],
  "author": "Bitovi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stealjs/steal-mocha/issues"
  },
  "homepage": "https://github.com/stealjs/steal-mocha",
  "devDependencies": {
    "testee": "^0.9.0"
  },
  "dependencies": {
    "mocha": "^6.2.0",
    "steal": "^1.11.0",
    "steal-css": "^1.3.2"
  }
}
