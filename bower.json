{
  "name": "steal-mocha",
  "version": "0.0.3",
  "main": "steal-mocha.js",
  "devDependencies": {},
  "ignore": [
    "**/.*",
    "package.json",
    "bower.json",
    "Gruntfile.js",
    "node_modules",
    "test"
  ],
  "dependencies": {
    "mocha": "~2.2.1"
  },
  "system": {
    "paths": {
      "mocha/mocha": "bower_components/mocha/mocha.js",
      "mocha/mocha.css": "bower_components/mocha/mocha.css"
    },
    "meta": {
      "mocha/mocha": {
        "format": "global",
        "exports": "mocha",
        "deps": [
          "steal-mocha/add-dom"
        ]
      }
    }
  }
}
